{
  "version": 2,
  "builds": [
    {
      "src": "/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/tips(.*)",
      "dest": "/routes/tips.js"
    },
    {
      "src": "/user(.*)",
      "dest": "/routes/user.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
